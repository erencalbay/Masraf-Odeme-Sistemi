    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PostgreSQL": "User ID=postgres;Password=asdasd123;Host=localhost;Port=5432;Database=ExpensePaymentSystem8;"
  },
  "TokenOptions": {
    "Audiences": [ "www.server.com" ],
    "Issuer": "www.server.com",
